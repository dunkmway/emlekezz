{
  "keepExistingTerminalsOpen": false,
  "runOnStartup": false,
  "terminals": [
    {
      "splitTerminals": [
        {
          "name": "ollama",
          "commands": ["cd development", "docker compose up"]
        }
      ]
    },
    {
      "splitTerminals": [
        {
          "name": "client",
          "commands": ["cd client", "npm run dev"]
        },
        {
          "name": "server",
          "commands": ["npm run supabase:start", "npm run supabase:serve"]
        }
      ]
    },
    {
      "splitTerminals": [
        {
          "name": "working"
        }
      ]
    }
  ]
}
